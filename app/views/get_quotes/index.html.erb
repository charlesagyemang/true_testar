
<br />
<div class="full" >
  <%= render 'testowners/ownerdash' %>
    <div class="col-sm-9">
      <div class="bs-callout bs-callout-danger shade_oneee full">
        <%= link_to "","#myModal",
 							{ id:"modal", data: {toggle: "modal"}} %>
     </div>
</div>
</div>

    <div class="modal fade" id="myModal" role="dialog">
           <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
          <div class="modal-header">
          </div>
          <div class="modal-body">
            <div class="bs-callout bs-callout-danger shade_oneee full">
               <div class="cost">

               </div>
               <h4>Number Of Testers</h4>
               <label><input type="radio" name="optradionum" value = 10>10</label>
               <label><input type="radio" name="optradionum" value= 50> 50 </label>
               <label><input type="radio" name="optradionum" value= 100> 100 </label>
               <br /><br />

               <h4>  Level Of Expertise</h4>
               <label><input type="radio" name="optradioexp" value="Low"> Low </label><br />
               <label><input type="radio" name="optradioexp" value="Medium"> Medium</label><br />
               <label><input type="radio" name="optradioexp" value="High"> High</label><br />

               <h4> Location </h4>
               <label><input type="radio" name="optradioreg" value= 1.5 > 1-3 Regions </label><br />
               <label><input type="radio" name="optradioreg" value= 3> 4-6 Regions </label><br />
               <label><input type="radio" name="optradioreg" value= 4> 7-9 Regions </label><br />
               <label><input type="radio" name="optradioreg" value= 5> All The 10 Regions </label><br /><br />

               <h4> Test Duration </h4>
               <label><input type="radio" name="optradiodur" value= 2> 1-2 Days </label><br />
               <label><input type="radio" name="optradiodur" value= 4> 3-4 Days </label><br />
               <label><input type="radio" name="optradiodur" value= 6> 5-6 Days </label><br />
               <label><input type="radio" name="optradiodur" value= 7> 1 Week </label><br /><br />

               <input id="quote" type="button" class="btn btn-default" value="Get Quote"></input>

            </div>

        </div>
    </div>
<script>
$(document).ready(function(){
  $('#modal').get(0).click();
})
</script>
